module Hello = {
  def nil =
    \on_nil : Int .
    \on_cons : Int -> Int -> Int .
    on_nil

  def cons =
    \head : Int .
    \tail : Int -> (Int -> Int -> Int) -> Int .
    \on_nil : Int .
    \on_cons : Int -> Int -> Int .
    on_cons head (tail on_nil on_cons)

  def length =
    \list : Int -> (Int -> Int -> Int) -> Int .
    list
      0
      (\head
       \tail add 1 tail )

  def identity : forall a. a -> a = \x x



  def main = length (cons 1 (cons 2 nil))
}